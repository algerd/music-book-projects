<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" fx:id="artistGenreTable" prefHeight="400.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mvc.genre.controller.ArtistGenreTableController">
     <TableView fx:id="artistTableView" onMouseClicked="#onMouseClickTable" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="40.0">
        <columns>
         <TableColumn fx:id="rankColumn" minWidth="50.0" prefWidth="50.0" text="Rank" />
          <TableColumn fx:id="artistColumn" minWidth="100.0" prefWidth="100.0" text="Artist" />
          <TableColumn fx:id="ratingColumn" minWidth="100.0" prefWidth="100.0" text="Rating" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   <Label fx:id="titleLabel" text="Title" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="10.0" />
</AnchorPane>
